<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.numberONe.mapper.DeliveryMapper">
	<select id="findRpRecordPage" resultType="com.numberONe.entity.DeliveryFormMap">
		SELECT
		TOP (100000)*
		FROM
		DeliveryTable
		<where>
			<if test="#{}"
		</where>
	</select>
</mapper>